* SIMPLE LOOP (CASHNER, 2014-07-16)
* START AT 0, ADD INCREM UNTIL MAX IS REACHED
*
PRINTER	EQU	18 
	ORIG	1000	
START	IOC	0(PRINTER)
	ORIG	1010
	CON	0
	ORIG	1020
	CON	2
	ORIG	1030
	CON	11
	LDA	1010
1H	STA	1010
	CMPA	1030
	JL	2F
	JG	3F
2H	ADD	1020
	JMP	1B
3H	SUB	1020
3H	CHAR
	STA	2000
	STX	2001
	OUT	2000(PRINTER)
	HLT
	END	START
