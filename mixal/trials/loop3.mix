* SIMPLEST LOOP (CASHNER, 2014-07-16)
* FROM BEGIN VALUE, ADD INCREM UNTIL MAX IS REACHED
*
PRINTER	EQU	18 
	ORIG	1000	
START	IOC	0(PRINTER)
	ORIG	1010
	CON	0	begin
	ORIG	1020
	CON	2	increment
	ORIG	1030
	CON	11	max
	LDA	1010	n = begin
*
* LOOP AND TEST
*
1H	ADD	1020	n + increment
	CMPA	1030	
	JL	1B	if n < max, go to 1H
*
* PRINT HIGHEST NUMBER REACHED THAT IS LESS THAN MAX
*
	SUB	1020	if n > max, print n - increment
	CHAR
	STX	2000
	OUT	2000(PRINTER)
	HLT
	END	START
