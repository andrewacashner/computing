* SIMPLER LOOP (CASHNER, 2014-07-16)
* FROM BEGIN VALUE, ADD INCREM UNTIL MAX IS REACHED
*
PRINTER	EQU	18 
BEGIN	EQU	0
INCREM	EQU	2
MAX	EQU	11
*
	ORIG	1000	
START	IOC	0(PRINTER)
	ORIG	1001
	CON	BEGIN
	ORIG	1002
	CON	MAX
1H	STA	2000	
	INCA	INCREM
	CMPA	1002
	JL	1B
*
* PRINT HIGHEST NUMBER REACHED THAT IS LESS THAN MAX
	LDA	2000	
	CHAR
	STX	3000
	OUT	3000(PRINTER)
	HLT
	END	START
