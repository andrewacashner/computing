* TEST IF INTEGER Q IS BOTH EVEN AND BETWEEN MIN AND MAX (INCLUSIVE)
* AAC, 2014-10-11
*
Q	EQU	16
MIN	EQU	12
MAX	EQU	36
LPR	EQU	18
STATE	EQU	2000
MSG	EQU	STATE + 10 
	ORIG	3000
START	IOC	0(LPR)
	ENT1	=Q=
	DEC1	MIN
	J1N	BAD
	ENT1	MAX
	DEC1	=Q=
	J1N	BAD
	LDX	=Q=
	DIV	=2=
	ENT1	0
	JXZ	PRINT
BAD	ENT1	1
PRINT	LDX	STATE,1
	STX	MSG
	OUT	MSG(LPR)
	HLT
	ORIG	STATE
	CON	"GOOD"
	CON	"BAD"
	END	START

